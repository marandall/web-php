  <announcements>
    <announcement lang="en">The PHP development team announces the immediate availability of PHP 7.1.23. This is a bugfix release.</announcement>
  </announcements>
  <tags/>
  <changes>
    <module id="Core">
      <change>
        <description>(method_exists on SPL iterator passthrough method corrupts memory).</description>
        <references>
          <reference type="bugfix">76901</reference>
        </references>
      </change>
      <change>
        <description>(Segfault in shutdown function after memory limit error).</description>
        <references>
          <reference type="bugfix">76846</reference>
        </references>
      </change>
    </module>
    <module id="CURL">
      <change>
        <description>(Use curl_multi_wait() so that timeouts are respected).</description>
        <references>
          <reference type="bugfix">76480</reference>
        </references>
      </change>
    </module>
    <module id="iconv">
      <change>
        <description>(iconv_mime_encode Q-encoding longer than it should be).</description>
        <references>
          <reference type="bugfix">66828</reference>
        </references>
      </change>
    </module>
    <module id="Opcache">
      <change>
        <description>(ZendOPcache.MemoryBase periodically deleted by the OS).</description>
        <references>
          <reference type="bugfix">76832</reference>
        </references>
      </change>
    </module>
    <module id="POSIX">
      <change>
        <description>(posix_getgrnam fails to print details of group).</description>
        <references>
          <reference type="bugfix">75696</reference>
        </references>
      </change>
    </module>
    <module id="Reflection">
      <change>
        <description>(Wrong exception being thrown when using ReflectionMethod).</description>
        <references>
          <reference type="bugfix">74454</reference>
        </references>
      </change>
    </module>
    <module id="Standard">
      <change>
        <description>(Wrong error message when fopen FTP wrapped fails to open data connection).</description>
        <references>
          <reference type="bugfix">73457</reference>
        </references>
      </change>
      <change>
        <description>(Bindto IPv6 works with file_get_contents but fails with stream_socket_client).</description>
        <references>
          <reference type="bugfix">74764</reference>
        </references>
      </change>
      <change>
        <description>(array_reduce is slow when $carry is large array).</description>
        <references>
          <reference type="bugfix">75533</reference>
        </references>
      </change>
    </module>
    <module id="Zlib">
      <change>
        <description>(php_zlib_inflate_filter() may not update bytes_consumed).</description>
        <references>
          <reference type="bugfix">75273</reference>
        </references>
      </change>
    </module>
  </changes>
</release>
