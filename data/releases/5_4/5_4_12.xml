  <changes>
    <module id="Core">
      <change>
        <description>(Wrong TSRM usage in zend_register_class alias).</description>
        <references>
          <reference type="bugfix">64099</reference>
        </references>
      </change>
      <change>
        <description>(get_html_translation_table() output incomplete with HTML_ENTITIES and ISO-8859-1).</description>
        <references>
          <reference type="bugfix">64011</reference>
        </references>
      </change>
      <change>
        <description>(isset() inconsistently produces a fatal error on protected property).</description>
        <references>
          <reference type="bugfix">63982</reference>
        </references>
      </change>
      <change>
        <description>(Bad warning text from strpos() on empty needle).</description>
        <references>
          <reference type="bugfix">63943</reference>
        </references>
      </change>
      <change>
        <description>(Use after scope error in zend_compile).</description>
        <references>
          <reference type="bugfix">63899</reference>
        </references>
      </change>
      <change>
        <description>(Poor efficiency of strtr() using array with keys of very different length).</description>
        <references>
          <reference type="bugfix">63893</reference>
        </references>
      </change>
      <change>
        <description>(zend_std_compare_objects crash on recursion).</description>
        <references>
          <reference type="bugfix">63882</reference>
        </references>
      </change>
      <change>
        <description>(Magic methods called twice for unset protected properties).</description>
        <references>
          <reference type="bugfix">63462</reference>
        </references>
      </change>
      <change>
        <description>(fopen follows redirects for non-3xx statuses).</description>
        <references>
          <reference type="bugfix">62524</reference>
        </references>
      </change>
      <change>
        <description>Support BITMAPV5HEADER in getimagesize().</description>
        <references/>
      </change>
    </module>
    <module id="Date">
      <change>
        <description>(Performance improvements for various ext/date functions).</description>
        <references>
          <reference type="bugfix">63699</reference>
        </references>
      </change>
      <change>
        <description>Comparsion of incomplete DateTime causes SIGSEGV.</description>
        <references>
          <reference type="bugfix">55397</reference>
        </references>
      </change>
    </module>
    <module id="FPM">
      <change>
        <description>(php with fpm fails to build on Solaris 10 or 11).</description>
        <references>
          <reference type="bugfix">63999</reference>
        </references>
      </change>
    </module>
    <module id="Litespeed">
      <change>
        <description>(-Werror=format-security error in lsapi code).</description>
        <references>
          <reference type="bugfix">63228</reference>
        </references>
      </change>
    </module>
    <module id="sqlite3">
      <change>
        <description>(sqlite3::bindvalue and relative PHP functions aren't using sqlite3_*_int64 API).</description>
        <references>
          <reference type="bugfix">63921</reference>
        </references>
      </change>
    </module>
    <module id="PDO_OCI">
      <change>
        <description>(Multi-row BLOB fetches).</description>
        <references>
          <reference type="bugfix">57702</reference>
        </references>
      </change>
      <change>
        <description>(Segfault in PDO_OCI on cleanup after running a long testsuite).</description>
        <references>
          <reference type="bugfix">52958</reference>
        </references>
      </change>
    </module>
    <module id="PDO_sqlite">
      <change>
        <description>(PDO::PARAM_INT casts to 32bit int internally even on 64bit builds in pdo_sqlite).</description>
        <references>
          <reference type="bugfix">63916</reference>
        </references>
      </change>
    </module>
  </changes>
