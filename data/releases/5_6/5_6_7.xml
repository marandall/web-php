        <references>
          <reference type="bugfix">67403</reference>
        </references>
      </change>
      <change>
        <description>(Inconsistent stream crypto values across versions) (Daniel Lowrey)</description>
        <references>
          <reference type="bugfix">69195</reference>
        </references>
      </change>
