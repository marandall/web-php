<?xml version="1.0"?>
<release version="1" id="5.6.3">
  <release>5.6.3</release>
  <date>13 Nov 2014</date>
  <source>
    <source>
      <name>PHP 5.6.3 (tar.bz2)</name>
      <filename>php-5.6.3.tar.bz2</filename>
