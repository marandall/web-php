  <announcements>
    <announcement lang="en">The PHP development team announces the immediate availability of PHP 7.2.20. This is a bugfix release.</announcement>
  </announcements>
  <tags/>
  <changes>
    <module id="Core">
      <change>
        <description>(Interface gets skipped if autoloader throws an exception).</description>
        <references>
          <reference type="bugfix">76980</reference>
        </references>
      </change>
    </module>
    <module id="DOM">
      <change>
        <description>(segfault when accessing properties of DOMDocumentType).</description>
        <references>
          <reference type="bugfix">78025</reference>
        </references>
      </change>
    </module>
    <module id="MySQLi">
      <change>
        <description>(When mysqli.allow_local_infile = Off, use a meaningful error message).</description>
        <references>
          <reference type="bugfix">77956</reference>
        </references>
      </change>
      <change>
        <description>(bindParam incorrect processing of bool types).</description>
        <references>
          <reference type="bugfix">38546</reference>
        </references>
      </change>
    </module>
    <module id="Opcache">
      <change>
        <description>(Path resolution fails if opcache disabled during request).</description>
        <references>
          <reference type="bugfix">78106</reference>
        </references>
      </change>
    </module>
    <module id="OpenSSL">
      <change>
        <description>(openssl_encrypt_ccm.phpt fails with OpenSSL 1.1.1c).</description>
        <references>
          <reference type="bugfix">78079</reference>
        </references>
      </change>
    </module>
    <module id="Sockets">
      <change>
        <description>(Socket_select fails when resource array contains references).</description>
        <references>
          <reference type="bugfix">78038</reference>
        </references>
      </change>
    </module>
    <module id="Standard">
      <change>
        <description>(Extract with EXTR_SKIP should skip $this).</description>
        <references>
          <reference type="bugfix">77135</reference>
        </references>
      </change>
      <change>
        <description>(preg_match failed).</description>
        <references>
          <reference type="bugfix">77937</reference>
        </references>
      </change>
    </module>
    <module id="Zip">
      <change>
        <description>(zip.h not found).</description>
        <references>
          <reference type="bugfix">76345</reference>
        </references>
      </change>
    </module>
  </changes>
</release>
